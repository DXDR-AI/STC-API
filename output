      writer = csv.writer(f) 
      writer.writerow(["id", "title",   "keywords", "desc", "desc_clean"]) 
      for i in range(l, total_pages + 1): 
          data = get_data('https://images-api.nasa.gov/search?q=nebula&media_ type=image&page={}'.format(i))   # Search input: nebula 
